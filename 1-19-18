;; The first three lines of this file were inserted by DrRacket. They record metadata
;; about the language level of this file in a form that our tools can easily process.
#reader(lib "htdp-advanced-reader.ss" "lang")((modname 1-19-18) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #t #t none #f () #f)))
(require picturing-programs)
(define-struct np (n posn))
(define TESTNP (make-np 1 (make-posn 2 3)))
(define (np-set-num np num)
  (make-np num (np-posn np)))
(check-expect (np-set-num TESTNP 3)
              (make-np 3 (make-posn 2 3)))
(define (np-set-posn np posn)
  (make-np (np-n np)
           posn))
(check-expect (np-set-posn TESTNP (make-posn 4 5))
              (make-np 1 (make-posn 4 5)))